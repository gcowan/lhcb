<RapidFit>
	<ParameterSet>
		// True Physics Parameters for signal

		<PhysicsParameter>
			<Name>F_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>delta_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>Csp</Name>
			<Value>1.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mistagP1</Name>
			<Value>1.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagP0</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagSetPoint</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP1</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP0</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaSetPoint</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.6653</Value>
			<Minimum>0.6</Minimum>
			<Maximum>0.75</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.0</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.15</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.249</Value>
			<Minimum>0.</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.5213</Value>
			<Type>Free</Type>
			<Minimum>0.</Minimum>
			<Maximum>0.8</Maximum>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>3.3</Value>
			<StepSize>0.05</StepSize>
			<Maximum>6.3</Maximum>
			<Minimum>-6.3</Minimum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>3.07</Value>
			<Minimum>-6.3</Minimum>
			<Maximum>6.3</Maximum>
			<StepSize>0.05</StepSize>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Minimum>-3.3</Minimum>
			<Maximum>3.3</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.8</Value>
			<Type>Free</Type>
			<Unit>ps-1</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>0.07</Value>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>lambda</Name>
			<Value>1.</Value>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

    	</ParameterSet>


	//==========================================
	// Specify which minimiser to use.

	<Minimiser>
		<MinimiserName>Minuit</MinimiserName>
		<MaxSteps>100000</MaxSteps>
		<GradTolerance>0.01</GradTolerance>
		<Quality>0</Quality>
	</Minimiser>

	<FitFunction>
		<FunctionName>NegativeLogLikelihoodThreaded</FunctionName>
		<Threads>8</Threads>
		<SetIntegratorTest>False</SetIntegratorTest>
		<OffSetNLL>True</OffSetNLL>
	</FitFunction>

	<NumberRepeats>1</NumberRepeats>

	<CommonPDF>
		<PDF>
			<Name>Bs2JpsiPhi_Signal_v7</Name>
			<ConfigurationParameter>UseHelicityBasis:False</ConfigurationParameter>
                        <ConfigurationParameter>AngularAcceptanceIgnoreNumerator:True</ConfigurationParameter>			
			<ConfigurationParameter>UseTimeAcceptance:False</ConfigurationParameter>
			<ConfigurationParameter>UseEventResolution:False</ConfigurationParameter>
			<ConfigurationParameter>ResolutionModel:DummyResolutionModel</ConfigurationParameter>
		</PDF>
	</CommonPDF>

	<CommonPhaseSpace>
		<PhaseSpaceBoundary>
			<Observable>
				<Name>time</Name>
				<TF1>B_s0_TRUETAU*1000</TF1>
				<Minimum>0.3</Minimum>
				<Maximum>14.0</Maximum>
				<Unit>ps</Unit>
			</Observable>
			<Observable>
                                <Name>helcosthetaK</Name>
                                <TF1>helcosthetaK</TF1>
                                <Minimum>-1.0</Minimum>
                                <Maximum>1.0</Maximum>
                                <Unit> </Unit>
                        </Observable>
                        <Observable>
                                <Name>helphi</Name>
                                <TF1>(helphi+TMath::Pi()-(helphi>0.)*(2.*TMath::Pi()))</TF1>
                                <Minimum>-3.1416</Minimum>
                                <Maximum>3.1416</Maximum>
                                <Unit>rad</Unit>
                        </Observable>
                        <Observable>
                                <Name>helcosthetaL</Name>
                                <TF1>helcosthetaL</TF1>
                                <Minimum>-1.0</Minimum>
                                <Maximum>1.0</Maximum>
                                <Unit> </Unit>
                        </Observable>
			<Observable>
				<Name>cosTheta</Name>
				<TF1>cosTheta</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>phi</Name>
				<TF1>phi</TF1>
				<Minimum>-3.1416</Minimum>
				<Maximum>3.1416</Maximum>
				<Unit>rad</Unit>
			</Observable>
			<Observable>
				<Name>cosPsi</Name>
				<TF1>cosPsi</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>tag</Name>
				#<TF1>B_s0_TRUEID/531.</TF1>
				<TF1>0.</TF1>
				<Value>1.</Value>
				<Value>0.</Value>
				<Value>-1.</Value>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>mistag</Name>
				<TF1>0.</TF1>
				<Minimum>0.</Minimum>
				<Maximum>0.5</Maximum>
				<Unit> </Unit>
			</Observable>
		</PhaseSpaceBoundary>
	</CommonPhaseSpace>

	<ToFit>
		<CommonPDF>True</CommonPDF>
		<DataSet>
			<Source>File</Source>
			<FileName>/Disk/speyside8/lhcb/gcowan1/MC2012_sim08e_Bs2JpsiPhi_DG0_minimal_truth_with_IPz_weights.root</FileName>
                        <CutString>(30. > TMath::Abs(phi_1020_M - 1020.)) && (16. > phi_1020_ENDVERTEX_CHI2/phi_1020_ENDVERTEX_NDOF) && (0.12 > B_s0_LOKI_DTF_CTAUERR/0.299792458) && (B_s0_LOKI_DTF_CHI2NDOF > 0) && (B_s0_LOKI_DTF_VCHI2NDOF > 0) && (5. > B_s0_LOKI_DTF_CHI2NDOF) && (10. > B_s0_ENDVERTEX_CHI2/B_s0_ENDVERTEX_NDOF) && (25. > B_s0_IPCHI2_OWNPV) && (B_s0_MINIPCHI2NEXTBEST > 50 || 0 > B_s0_MINIPCHI2NEXTBEST) && (60 > TMath::Abs(J_psi_1S_MM - 3090))&& (Kplus_PIDK > 0.) && (Kminus_PIDK > 0.)</CutString>
			<NumberEvents>100000000</NumberEvents>

			<CommonPhaseSpace>
			</CommonPhaseSpace>

		</DataSet>
	</ToFit>

</RapidFit>


